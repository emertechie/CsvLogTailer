<?xml version="1.0" encoding="utf-16"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>CsvLogTailer</id>
		<version>0.0.0</version>
		<title>CsvLogTailer</title>
		<authors>Andrew Smith</authors>
		<owners>Andrew Smith</owners>
		<projectUrl>https://github.com/emertechie/CsvLogTailer</projectUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>Utility to tail a CSV log file, or a folder containing CSV log files. Exposes results as an IObservable&lt;LogRecord&gt;. Supports detecting new files, and only tailing from previous position.</description>
		<copyright>Copyright 2012</copyright>
		<dependencies>
			<dependency id="CsvParser" version="0.5" />
			<dependency id="Rx-Core" version="2.0" />
			<dependency id="Rx-Linq" version="2.0" />
		</dependencies>
	</metadata>
	<files>
		<file src="_build\bin\*.dll" target="lib\net40" />
		<file src="_build\bin\*.pdb" target="lib\net40" />
		<file src="_build\bin\*.xml" target="lib\net40" />
	</files>
</package>
